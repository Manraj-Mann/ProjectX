# Configuration file example for testing ConfigReader

[General]
appName=MyApplication
version=1.0.0
debugMode=true
maxConnections=100
timeout=30.5

[Database]
host=localhost
port=5432
username=admin
password=secret
database=my_database

[Logging]
logLevel=INFO
logFilePath=/var/log/myapp.log
maxFileSize=10485760  # 10 MB in bytes

[Server]
ipAddress=192.168.1.1
port=8080
useSSL=true

[Features]
enableFeatureX=false
enableFeatureY=true
maxRetryAttempts=5

[Advanced]
maxThreads=8
cacheSizeMB=256
enableCompression=true
compressionLevel=3

[CustomSection]
customKey1=value1
customKey2=value2

[Extra]
nestedBlock1=value1
nestedBlock2=value2

# End of configuration file
